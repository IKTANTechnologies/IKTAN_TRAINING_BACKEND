    padding: 0.3% 0 0.3% 0;
    }

    .section .ocupacion {
    width: 49.4%;
    border: 1px solid black;

    border-right: 1px solid black;
    border-bottom: none;
    
    }

    .section2 {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    }

    .datos-curp {
    width: 50%;
    background-color: rgb(255, 255, 255);
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    //- border: 1px solid black;
    border-left: 1px solid black;
    border-top: 1px solid black;
    border-bottom: none; 
    }

    .section2 .datos-curp .dato {
